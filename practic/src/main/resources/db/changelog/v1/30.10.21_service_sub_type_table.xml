<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="service_subtype_1" author="Thor">
        <createTable tableName="service_sub_type">
            <column name="id" type="int" autoIncrement="true" >
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_subtype_id" />
            </column>
            <column name="name" type="varchar(100)"></column>
        </createTable>
    </changeSet>

    <changeSet id="service_subtype_2" author="Thor">
        <insert tableName="service_sub_type">
            <column name="name">Chassis</column>
        </insert>
        <insert tableName="service_sub_type">
            <column name="name">Engine</column>
        </insert>
        <insert tableName="service_sub_type">
            <column name="name">Brake system</column>
        </insert>
        <insert tableName="service_sub_type">
            <column name="name"> Transmission</column>
        </insert>
    </changeSet>

</databaseChangeLog>